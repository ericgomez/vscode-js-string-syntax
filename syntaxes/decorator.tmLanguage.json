{
  "scopeName": "source.ts.decorator",
  "injectionSelector": "L:source.js -comment -(string -meta.embedded) -meta.jsx -source.jsx -source.js.jsx -source.tsx, L:source.ts -comment -(string -meta.embedded) -meta.jsx -source.jsx -source.js.jsx -source.tsx",
  "patterns": [
    {
      "match": "(?m)^[ \\t]*(@)([a-zA-Z0-9_]+)\\b",
      "captures": {
        "1": {
          "name": "punctuation.decorator.ts entity.name.decorator.structure.ts"
        },
        "2": {
          "name": "entity.name.function.ts entity.name.decorator.structure.ts"
        }
      }
    }
  ],
  "name": "Decorator Structure"
}