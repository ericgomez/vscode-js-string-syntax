{
  "name": "js-string-syntax",
  "displayName": "JS/TS String Syntax",
  "description": "Syntax highlighting for JavaScript and TypeScript strings containing HTML, XML/SVG, SQL, and CSS",
  "version": "0.1.11",
  "publisher": "ericgomez",
  "icon": "images/logo.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/inference.tmLanguage.json",
        "scopeName": "source.js.inference",
        "injectTo": [
          "source.js",
          "source.ts"
        ],
        "embeddedLanguages": {
          "text.html.embedded.js": "html",
          "source.sql.embedded.js": "sql",
          "text.xml.embedded.js": "xml"
        }
      },
      {
        "path": "./syntaxes/selector.tmLanguage.json",
        "scopeName": "source.js.selector",
        "injectTo": [
          "source.js",
          "source.ts",
          "source.jsx",
          "source.tsx",
          "source.js.jsx"
        ]
      },
      {
        "path": "./syntaxes/js-xml.tmLanguage.json",
        "scopeName": "source.js.xml",
        "injectTo": [
          "source.js",
          "source.ts",
          "source.jsx",
          "source.tsx",
          "source.js.jsx"
        ]
      },
      {
        "path": "./syntaxes/js-html.tmLanguage.json",
        "scopeName": "source.js.html",
        "injectTo": [
          "source.js",
          "source.ts",
          "source.jsx",
          "source.tsx",
          "source.js.jsx"
        ]
      },
      {
        "path": "./syntaxes/js-sql.tmLanguage.json",
        "scopeName": "source.js.sql",
        "injectTo": [
          "source.js",
          "source.ts",
          "source.jsx",
          "source.tsx",
          "source.js.jsx"
        ]
      },
      {
        "path": "./syntaxes/js-css.tmLanguage.json",
        "scopeName": "source.js.css",
        "injectTo": [
          "source.js",
          "source.ts",
          "source.jsx",
          "source.tsx",
          "source.js.jsx"
        ]
      },
      {
        "path": "./syntaxes/decorator.tmLanguage.json",
        "scopeName": "source.ts.decorator",
        "injectTo": [
          "source.js",
          "source.ts"
        ]
      }
    ]
  },
  "keywords": [
    "javascript",
    "typescript",
    "html",
    "sql",
    "css",
    "xml",
    "svg",
    "highlight",
    "string",
    "syntax",
    "template",
    "template string",
    "template literal",
    "embedded html",
    "inline html",
    "html in js",
    "html in ts",
    "css in ts",
    "sql in js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ericgomez/vscode-js-string-syntax.git"
  },
  "bugs": {
    "url": "https://github.com/ericgomez/vscode-js-string-syntax/issues",
    "email": "eric.gomez.sanchez@gmail.com"
  }
}